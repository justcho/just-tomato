  .clock { 
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
  }
  .clock text {
    color: #fff;
    font-size: 25rpx;
    position: absolute;
    top: 51%;
    left: 50%;
    transform: translate(-50%,-80%);
  }
  .clock image { 
    width: 320rpx;
    height: 320rpx;
  }
  